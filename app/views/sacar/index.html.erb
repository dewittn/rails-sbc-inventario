<% page_title "Para Sacar" %>
<h1><%= yield :title %></h1>
<% unless @inventarios.empty? %>
	<table class="results">
	  <tr class="title">
	  	<th>C&oacute;digo</th>	
	    <th>Fila</th>
	    <th>Columna</th>		
	    <th>Cantidad</th>
		<th>Nombre</th>
		<th>Numero</th>
	    <th>Marca</th>	
	    <th>Tipo</th>	
	    <th>Color</th>	
	    <th>Talla</th>
		<th></th>
		<th></th>
	  </tr>
	<%= render :partial => 'inventario', :collection => @inventarios %>
	</table>
	<%= will_paginate @inventarios %>
<% else %>
	<p style="color: green"><%= "No se encontraron registros" %></p>
<% end %>

